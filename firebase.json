{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
	  "YetFilledIn.md",
	  "README.old.md"
    ],
	"rewrites": [
		{
		  "source": "**",
		  "destination": "/index.html"
		},
		{ 
			"source": "/api/**", 
			"function": "app" 
		}
	]
  }
}
